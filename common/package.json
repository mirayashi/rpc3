{
  "name": "rpc3-common",
  "version": "1.0.0",
  "description": "Common library for all components of the rpc3 project",
  "exports": {
    "types": "./dist/src/index.d.ts",
    "import": "./dist/src/index.js",
    "require": "./dist/src/index.js"
  },
  "scripts": {
    "build": "npm run copy-generated && tsc",
    "clean": "rimraf ./generated ./dist",
    "copy-generated": "rimraf ./generated && copyfiles -EVu 3 ../sc/typechain-types/common.ts ../sc/typechain-types/contracts/RPC3/RPC3.ts ../sc/typechain-types/factories/contracts/RPC3/RPC3__factory.ts generated"
  },
  "keywords": [
    "common",
    "rpc3"
  ],
  "author": "Mirayashi",
  "license": "MIT"
}
